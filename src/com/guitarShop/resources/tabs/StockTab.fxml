<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.Pane?>

<Pane maxHeight="-Infinity" maxWidth="-Infinity"
      minHeight="-Infinity" minWidth="-Infinity"
      prefHeight="750.0" prefWidth="1000.0"
      xmlns="http://javafx.com/javafx/11.0.1"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.guitarShop.java.controllers.tabControllers.StockTabController">
   <children>
      <TableView fx:id="stockTable" layoutX="28.0" layoutY="46.0" prefHeight="496.0" prefWidth="935.0">
         <columns>
            <TableColumn fx:id="manufacturerCol" prefWidth="125.0" text="Manufacturer" />
            <TableColumn fx:id="modelCol" prefWidth="103.0" text="Model" />
            <TableColumn fx:id="priceCol" prefWidth="93.0" text="Price" />
            <TableColumn fx:id="typeCol" prefWidth="104.0" text="Type" />
            <TableColumn fx:id="pickupsCol" prefWidth="118.0" text="Pickups" />
            <TableColumn fx:id="bridgeCol" prefWidth="118.0" text="Bridge" />
            <TableColumn fx:id="stringsCol" prefWidth="60.0" text="Strings" />
            <TableColumn fx:id="tunersCol" prefWidth="127.0" text="Tuners" />
            <TableColumn fx:id="quantityCol" prefWidth="86.0" text="In stock" />
         </columns></TableView>
      <Button fx:id="refreshStockTableButton" layoutX="114.0" layoutY="576.0" mnemonicParsing="false" onAction="#initTable" prefHeight="30.0" prefWidth="142.0" text="Refresh" />
      <Button fx:id="editButton" layoutX="268.0" layoutY="576.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="142.0" text="Edit" />
      <Button fx:id="deleteButton" layoutX="424.0" layoutY="576.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="142.0" text="Delete" />
      <Button fx:id="addButton" layoutX="579.0" layoutY="576.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="142.0" text="Add" />
      <Button fx:id="viewButton" layoutX="735.0" layoutY="576.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="142.0" text="View" />
   </children>
</Pane>
